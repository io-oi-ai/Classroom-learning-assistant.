   {
     "version": 2,
     "github": {
       "enabled": false
     },
     "builds": [
       { "src": "package.json", "use": "@vercel/next" }
     ],
     "rewrites": [
       { "source": "/api/(.*)", "destination": "https://classroom-learning-assistant-backend.onrender.com/api/$1" }
     ],
     "env": {
       "NEXT_PUBLIC_API_URL": "https://classroom-learning-assistant-backend.onrender.com"
     }
   }
